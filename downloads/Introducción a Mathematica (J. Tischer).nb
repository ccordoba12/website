Cell["\<\
2. Buscar una soluci\[OAcute]n del problema en valores de frontera y''[t]= 5 \
Sinh[5 y[t]], y[0]==0, y[1]==1.\
\>", "Text"],

Cell[TextData[" "], "Solutions"],

Cell[BoxData[
    \(\(Clear[y]; \)\)], "Input"],

Cell[BoxData[
    RowBox[{\(f[a_]\), ":=", 
      RowBox[{
        RowBox[{"(", 
          RowBox[{"y", "/.", 
            RowBox[{"NDSolve", "[", 
              RowBox[{
                RowBox[{"{", 
                  RowBox[{
                    RowBox[{
                      RowBox[{
                        SuperscriptBox["y", "\[DoublePrime]",
                          MultilineFunction->None], "[", "t", "]"}], "==", 
                      \(5\ Sinh[5\ y[t]]\)}], ",", \(y[0] == 0\), ",", 
                    RowBox[{
                      RowBox[{
                        SuperscriptBox["y", "\[Prime]",
                          MultilineFunction->None], "[", "0", "]"}], "==", 
                      "a"}]}], "}"}], ",", "y", ",", \({t, 0, 1}\)}], "]"}]}],
           ")"}], "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}]], 
  "Input"],

Cell[BoxData[
    \(f[1]\)], "Input"],

Cell[BoxData[
    \(%\[LeftDoubleBracket]1, 1, 2\[RightDoubleBracket]\)], "Input"],

Cell[BoxData[
    \(g[a_] := 
      With[{s = f[a]}, s[s\[LeftDoubleBracket]1, 1, 2\[RightDoubleBracket]]]
        \)], "Input"],

Cell[BoxData[
