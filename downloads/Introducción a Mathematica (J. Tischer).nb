Cell[BoxData[
    \({x1, x2, x3, x4, x5} = x /. %\)], "Input"],

Cell[BoxData[
    \(x2 < x3 < \[Pi]\)], "Input"],

Cell[TextData[
"La funci\[OAcute]n If de Mathematica puede tener 2,3 o 4 variables."], "Text"],

Cell[BoxData[
    \(If[1 == 1, Plot[Sin[x], {x, 0, \[Pi]}]]\)], "Input"],

Cell[BoxData[
    \(If[1 \[NotEqual] 1, Plot[Sin[x], {x, 0, \[Pi]}]]\)], "Input"],

Cell[BoxData[
